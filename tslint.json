{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [true, "never-prefix"],
        "no-unused-variable": true,
        "object-literal-sort-keys": false,
        "quotemark": [true, "single"],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-rest-spread",
            "check-type",
            "check-typecast",
            "check-type-operator",
            "check-preblock"
        ],
        "no-console": false,
        "trailing-comma": false,
        "no-shadowed-variable": false
    },
    "rulesDirectory": []
}
